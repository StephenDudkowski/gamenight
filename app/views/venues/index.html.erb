<p id="notice"><%= notice %></p>

<h1>VENUES!</h1>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Website</th>
      <th>Type</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @v.each do |x| %>
      <tr>
        <td><%= x.name %></td>
        <td><%= x.address %></td>
        <td><%= x.website %></td>
        <td><%= x.venues_type %></td>
        <td><%= x.phone %></td>
        <td><%= link_to 'Show', x %></td>
        <td><%= link_to 'Edit', edit_venue_path(x) %></td>
        <td><%= link_to 'Destroy', x, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>